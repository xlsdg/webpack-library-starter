    "babel-preset-es2015": "6.18.0",
    "chai": "3.5.0",
    "eslint": "3.13.1",
    "eslint-loader": "1.6.1",
    "mocha": "3.2.0",
    "webpack": "1.14.0",
    "yargs": "6.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/krasimir/webpack-library-starter.git"
